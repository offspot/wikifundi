{
  "sites": {
    "src": {
      "fam": "wikipedia",
      "code": "fr"
    },
    "dst": {
      "fam": "kiwix",
      "code": "kiwix"
    }
  },
  "pages": [
    "MediaWiki:Common.js",
    "MediaWiki:Common.css",
    "MediaWiki:Vector.css"
  ],
  "categories": [
    {
      "title": "Projet_WikiFundi_Contenu"
    }
  ],
  "modifications": [
    {
      "pages": [
        "^Modèle:Coord$",
        "^Modèle:Commons$",
        "^Modèle:Commons-inline$",
        "^Modèle:Wikidata$",
        "^Modèle:WikidataOI$",
        "^Module:Wd$"
      ],
      "empty": true
    },
    {
      "pages": [
        "^Modèle:Portail$"
      ],
      "empty": true
    },
    {
      "pages": [
        "^MediaWiki:Common.css$"
      ],
      "substitutions": [
        {
          "pattern": ".mw-wiki-logo.*?}",
          "repl": "/* keep Wikifundi Logo  */"
        }
      ]
    },
    {
      "pages": [
        "^Main Page$"
      ],
      "delete": true
    },
    {
      "pages": [
        "^Module:Coordinates$"
      ],
      "substitutions": [
        {
          "pattern": "=wikidatacat\\(globe\\)",
          "repl": "={} -- Wikifundi not use Wikidata"
        },
        {
          "pattern": "if maplink then.*?end",
          "repl": "-- Wikifundi not use Wikidata"
        },
        {
          "pattern": "if wikidata == \\'true\\' then",
          "repl": "if false then -- Wikifundi not use Wikidata"
        }
      ]
    },
    {
      "pages": [
        "^Module:Bandeau$",
        "^Module:Wikidata$",
        "^Module:Autorité$"
      ],
      "substitutions": [
        {
          "pattern": "mw.wikibase.getEntity\\(.*?\\)",
          "repl": "nil -- Wikifundi not use Wikidata"
        }
      ]
    },
    {
      "pages": [
        "^Module:Infobox/Localdata$"
      ],
      "substitutions": [
        {
          "pattern": "wikidata.getEntity\\(localdata.wikidata\\)",
          "repl": "nil -- Wikifundi not use Wikidata"
        }
      ]
    },
    {
      "pages": [
        "^Module:Infobox$"
      ],
      "substitutions": [
        {
          "pattern": "wd.getClaims\\(query\\)",
          "repl": "nil -- Wikifundi not use Wikidata"
        }
      ]
    },
    {
      "categories": [
        {
          "title": "Projet_WikiFundi_Contenu"
        }
      ],
      "substitutions": [
        {
          "pattern": "\\[\\[(Category|Catégorie):Projet( |_)WikiFundi( |_|)(Contenu|)\\]\\]",
          "repl": " "
        },
        {
          "pattern": "<!--2BREINSERTED(.*)-->",
          "repl": "\\1"
        }
      ]
    }
  ]
}
